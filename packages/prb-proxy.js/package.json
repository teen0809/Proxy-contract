{
  "name": "prb-proxy.js",
  "description": "JavaScript SDK for the PRBProxy proxy contract to compose Ethereum transactions on behalf of the owner",
  "version": "1.0.0",
  "author": {
    "name": "Paul Razvan Berg",
    "url": "https://paulrberg.com"
  },
  "bugs": {
    "url": "https://github.com/paulrberg/prb-proxy/issues"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.4.1",
    "@ethersproject/address": "^5.4.0",
    "@ethersproject/keccak256": "^5.4.0"
  },
  "devDependencies": {
    "@ethersproject/bytes": "^5.4.0",
    "@ethersproject/constants": "^5.4.0",
    "@ethersproject/providers": "^5.4.5",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/mocha-each": "^2.0.0",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "earljs": "^0.1.10",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "ethers": "^5.4.7",
    "istanbul-reports": "^3.0.2",
    "mocha": "^9.1.2",
    "mocha-each": "^2.0.1",
    "nyc": "^15.1.0",
    "nyc-report-lcov-absolute": "^1.0.0",
    "prettier": "^2.4.1",
    "rollup": "^2.58.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "shx": "^0.3.3",
    "source-map-support": "^0.5.20",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "files": [
    "/dist/**/*.d.ts",
    "/dist/**/*.d.ts.map",
    "/dist/**/*.js",
    "/dist/**/*.js.map",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/paulrberg/prb-proxy/tree/main/packages/prb-proxy.js#readme",
  "keywords": [
    "blockchain",
    "delegatecall",
    "ethereum",
    "prb-proxy",
    "proxy",
    "smart-contracts",
    "typescript"
  ],
  "license": "Unlicense",
  "main": "./dist/node/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages/prb-proxy.js",
    "type": "git",
    "url": "https://github.com/paulrberg/prb-proxy"
  },
  "scripts": {
    "build": "yarn build:node && yarn build:browser",
    "build:browser": "yarn rollup",
    "build:node": "tsc --project ./tsconfig.prod.json",
    "clean": "shx rm -rf ./.nyc_output ./coverage ./dist ./coverage.json ./tsconfig.prod.tsbuildinfo",
    "clean:node_modules": "yarn workspaces foreach --parallel run clean:node_modules && shx rm -rf ./node_modules",
    "coverage": "yarn nyc --nycrc-path ./.nycrc.yaml mocha",
    "lint": "yarn lint:ts && yarn prettier:check && yarn typecheck",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ../../.eslintignore --ext .js,.ts .",
    "prepack": "yarn build",
    "prettier": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore --write \"**/*.{js,json,md,ts,yaml,yml}\"",
    "prettier:check": "prettier --check --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"**/*.{js,json,md,ts,yaml,yml}\"",
    "rollup": "rollup --config ./rollup.config.ts",
    "test": "mocha --config ./.mocharc.yaml",
    "typecheck": "tsc --incremental false --noEmit"
  },
  "types": "./dist/node/index.d.ts"
}
